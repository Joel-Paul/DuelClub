; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Ability",
"language": "GDScript",
"path": "res://Cards/Abilities/ability.gd"
}, {
"base": "Node2D",
"class": "Card",
"language": "GDScript",
"path": "res://Cards/CardBlank/Card.gd"
}, {
"base": "Node2D",
"class": "Dueler",
"language": "GDScript",
"path": "res://Dueler/Dueler.gd"
} ]
_global_script_class_icons={
"Ability": "",
"Card": "",
"Dueler": ""
}

[application]

config/name="DuelClub"
run/main_scene="res://Arena/Arena.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://global.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[rendering]

quality/filters/msaa=2
environment/default_environment="res://default_env.tres"
environment/size/height=1080
environment/size/width=1920
